<% if logged_in? && current_user.likeMusic?(song) %>
    <a data-remote="true" rel="nofollow" data-method="delete" href="/songs/<%= song.id %>/like"><img src="unfire.png" height="40px;"></a>
<% else %>
    <a data-remote="true" rel="nofollow" data-method="post" href="/songs/<%= song.id %>/like"><img src="fire.png" height="40px;"></a>
<% end %>
<%= song.musiclikes.count %>
