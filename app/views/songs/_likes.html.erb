<% if logged_in? && current_user.likeMusic?(song) %>
<%= link_to (image_tag "unfire.png", style: "height:22px;"), song_like_path(song), class: "btn btn-info btn-sm buticons glyphicon glyphicon-fire", method: :delete, remote: true %>
<% else %>
<%= link_to (image_tag("fire.png", style:"height:22px;")), song_like_path(song), class: "btn btn-info btn-sm buticons glyphicon glyphicon-fire",method: :post, remote: true %>
<% end %>
