<div class="musicPlayer">
  <div class="songDetails">
    <div class="songCover">
      <img src="http://localhost:3000/fire.png" style="width:100%; height:100%;">
    </div>
    <p id="songTitle">Song Title</p>
    <p id="songSubGenre">Song Subgenre</p>
    <p id="songGenre">Song Genre</p>
  </div>
  <div class="player">
  <!--  <button class="play border border-dark" onclick="playPause()"><i id="play-icon" class="ion-play"></i></button>-->
    <center>
      <div id="currentTime" style="position:absolute; top: 5px; left: 5px; ">0:00</div>
        <div class="seek-bar">
 	        <div class="fill"></div>
 	        <div class="handle"></div>
 	      </div>
      <div id="duration" style="position:absolute; top: 5px; right: 5px; ;">4:20</div>
      <button class="play" onclick="playPause()"><i id="play-icon" class="ion-play"></i></button>
      <div class="volume">
        <input id="vol-control" type="range" min="0" max="100" step="1" oninput="SetVolume(this.value)" onchange="SetVolume(this.value)"></input>

      </div>
    </center>
    <div class="BCButton">
    </div>
