<span onclick="sendTheAJAX('home', '')" class="VibinTab">Vibin</span>
<span class="ExploreTab" onclick="sendTheAJAX('explore', '')">Explore</span>
<span class="GroupiesTab" id="G" onclick="sendTheAJAX('groupies','')">Groupies</span>

<%= form_tag search_path, method: :get, remote: true do %>
  <%= text_field_tag :term, nil, placeholder: "Search", class: "SearchBar" %>
<% end %>
<div style="float: right;">
<a class="MessagesTab" onclick="sendTheAJAX('conversations', '')">Messages</a>
<div class="NotificationTabDiv">
  <%= image_tag "fire.png", style: "height: 55px; width: 100%;" %>
</div>
<div class="userTabDiv">
    <span onclick="sendTheAJAX('<%= user_path(current_user) %>', '')"><%= image_tag "default-avatar.jpg", class: "search-ava" %></span>
</div>

<% if logged_in? %>
  <div id="current-user"></div>
<% end %>
</div>
